{
    "name": "citronpo1nt/eet",
    "description": "EET (Electronic records of sales for Czech Ministry of Finance) Client for PHP, based on ondrejnov/eet",
    "type": "library",
    "license": ["MIT", "BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
    "version": "0.3.0",
    "require": {
        "php": ">=5.6.0",
        "robrichards/xmlseclibs": "2.0.*",
        "robrichards/wse-php": "2.0.*",
        "ext-curl": "*",
        "ext-soap": "*",
        "ext-openssl": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.7.5"
    },
    "suggest": {
        "ramsey/uuid": "Useful tool for generating UUIDv4"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
          "Po1nt\\EET\\": "src/"
        }
    }
}
